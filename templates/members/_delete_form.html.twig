<form method="post" action="{{ path('members_delete', {'id': member.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ member.id) }}">
    <button class="btn btn-danger mt-3 py-2 mx-3">Supp. mon compte</button>
</form>
